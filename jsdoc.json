{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["lib/", "fields.js", "partials.js", "partials/"]
  },
  "templates": {
    "cleverLinks": true
  },
  "opts": {
    "template": "templates/default",
    "readme": "./README.md",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "idInHeadings": true
  }
}
